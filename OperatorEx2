public class OperatorEx2{
public static void main(String []args){
int a= 7/2;  //3
System.out.println(a);
a=-7/2;//-3
System.out.println(a);
a=-7/-2;//3
System.out.println(a);
a=7%2;//1
System.out.println(a);
a=-7%2;//-1
System.out.println(a);
a=7%-2;//1
System.out.println(a);
a=-7%-2;//-1
System.out.println(a);
}
}
